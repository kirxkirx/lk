#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(int argc,char **argv){
 double hh,mm,ss;
 if(argc<3){
  fprintf(stderr,"Usage: %s RA DEC\n",argv[0]);
  return 1;
 }
 sscanf(argv[1],"%lf:%lf:%lf",&hh,&mm,&ss); 
 hh+=mm/60+ss/3600;
 hh*=15;
 fprintf(stdout,"%12.7lf\n",hh);
 sscanf(argv[2],"%lf:%lf:%lf",&hh,&mm,&ss);
 if( hh>=0 && argv[2][0]!='-' )hh+=mm/60+ss/3600;
 if( hh<=0 && argv[2][0]=='-' )hh-=mm/60+ss/3600;
 fprintf(stdout,"%+12.7lf\n",hh);
 
 return 0;
}
